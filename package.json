{
  "name": "@holochain-playground/elements",
  "version": "0.0.1",
  "description": "Holochain playground that simulates real life holochain behaviour",
  "author": "guillem.cordoba@gmail.com",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "rimraf dist && tsc -w",
    "test": "karma start --coverage",
    "test:watch": "concurrently --kill-others --names tsc,karma \"npm run tsc:watch\" \"karma start --auto-watch=true --single-run=false\"",
    "analyze": "wca analyze --features cssproperty member slot event --outFile custom-elements.json",
    "storybook": "npm run build && npm run analyze && start-storybook -p 8001",
    "storybook:build": "build-storybook",
    "storybook:publish": "gh-pages -d dist",
    "publish-to-branch": "npm run build && gh-pages -d . -b ui-build",
    "build:publish": "npm run build && npm publish"
  },
  "dependencies": {
    "@alenaksu/json-viewer": "^0.1.0",
    "@holochain-open-dev/common": "git+https://github.com/holochain-open-dev/common.git#ui-build",
    "@holochain-open-dev/core-types": "git+https://github.com/holochain-open-dev/core-types.git#build",
    "@holochain-playground/container": "git+https://github.com/holochain-playground/container.git#build",
    "@holochain-playground/core": "git+https://github.com/holochain-playground/core.git#build",
    "@open-wc/scoped-elements": "^1.3.3",
    "cytoscape": "^3.17.0",
    "cytoscape-cola": "^2.3.0",
    "cytoscape-dagre": "^2.2.2",
    "cytoscape-euler": "^1.2.2",
    "cytoscape-klay": "^3.1.3",
    "cytoscape-popper": "^1.0.7",
    "cytoscape-spread": "^3.0.0",
    "lit-element": "^2.3.1",
    "lit-html": "^1.3.0",
    "lodash-es": "^4.17.15",
    "scoped-material-components": "git+https://github.com/guillemcordoba/scoped-material-components.git#build"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.0",
    "@open-wc/testing": "^2.0.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.2",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^5.0.2",
    "@storybook/addon-a11y": "6.0.0-rc.28",
    "@storybook/addon-actions": "6.0.0-rc.28",
    "@storybook/addon-backgrounds": "6.0.0-rc.28",
    "@storybook/addon-controls": "6.0.0-rc.28",
    "@storybook/addon-docs": "^6.0.0-rc.28",
    "@storybook/addon-events": "6.0.0-rc.28",
    "@storybook/addon-jest": "6.0.0-rc.28",
    "@storybook/addon-knobs": "6.0.0-rc.28",
    "@storybook/addon-links": "6.0.0-rc.28",
    "@storybook/addon-storyshots": "6.0.0-rc.28",
    "@storybook/addon-storysource": "6.0.0-rc.28",
    "@storybook/addon-viewport": "6.0.0-rc.28",
    "@storybook/addons": "6.0.0-rc.28",
    "@storybook/client-api": "6.0.0-rc.28",
    "@storybook/core": "6.0.0-rc.28",
    "@storybook/core-events": "6.0.0-rc.28",
    "@storybook/source-loader": "6.0.0-rc.28",
    "@storybook/web-components": "6.0.0-rc.28",
    "@types/lodash-es": "^4.17.3",
    "@web/test-runner": "^0.9.7",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "core-js": "^3.6.5",
    "deepmerge": "^3.2.0",
    "es-dev-server": "^1.5.0",
    "gh-pages": "^3.1.0",
    "rimraf": "^2.7.1",
    "rollup": "^2.6.0",
    "rollup-plugin-multi-input": "^1.1.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "storybook-addon-web-components-knobs": "^0.3.14",
    "temp-dir": "^2.0.0",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.7",
    "web-component-analyzer": "^1.1.6"
  }
}
